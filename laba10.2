include <iostream>
#include <cmath>

using namespace std;

double cal(int i) {
    if (i > 50) {
        return 1; 
    }

    double x = log10(i + 0.5);

    return x * cal(i + 1); 
}

int main() {
    setlocale(LC_ALL, "ru");
    double y = cal(1); 

    cout << "Произведение от i=1 до i=50 : " << y << endl;

    return 0;
}
